{
  "name": "solarstats",
  "version": "1.0.0",
  "description": "Official SolarTweaks Stats proxy",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "package": "npm run build && pkg .",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solar-Tweaks/SolarStats.git"
  },
  "keywords": [
    "proxy",
    "minecraft",
    "solarstats"
  ],
  "author": "RichardDorian",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Solar-Tweaks/SolarStats/issues"
  },
  "homepage": "https://github.com/Solar-Tweaks/SolarStats#readme",
  "dependencies": {
    "@solar-tweaks/minecraft-protocol-lunarclient": "^1.0.0",
    "hypixel-api-reborn": "^10.0.0",
    "minecraft-protocol": "^1.30.0",
    "prismarine-proxy": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "pkg": "^5.5.1",
    "typed-emitter": "^1.4.0",
    "typescript": "^4.5.4"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "node_modules/minecraft-packets/minecraft-packets/data/pc/1.8",
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist/"
  }
}
