{
  "name": "solarstats",
  "version": "1.0.0-dev",
  "description": "Official SolarTweaks Stats proxy",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generateConfigSchema": "typescript-json-schema src/Types.ts Config",
    "build": "tsc",
    "obfuscateStatsModule": "npm run build && node obfuscation.js",
    "package": "npm run obfuscateStatsModule && pkg . --compress GZip",
    "start": "node .",
    "st-install": "node ./scripts/install.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solar-Tweaks/SolarStats.git"
  },
  "keywords": [
    "proxy",
    "minecraft",
    "solarstats"
  ],
  "author": "RichardDorian",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Solar-Tweaks/SolarStats/issues"
  },
  "homepage": "https://github.com/Solar-Tweaks/SolarStats#readme",
  "dependencies": {
    "@solar-tweaks/minecraft-protocol-lunarclient": "github:Solar-Tweaks/minecraft-protocol-lunarclient",
    "axios": "^0.26.0",
    "hypixel-api-reborn": "^10.0.0",
    "jsonschema": "^1.4.0",
    "minecraft-protocol": "^1.32.0",
    "prismarine-proxy": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/uuid": "^8.3.4",
    "javascript-obfuscator": "^4.0.0",
    "pkg": "^4.4.9",
    "typed-emitter": "^1.4.0",
    "typescript": "^4.5.4",
    "typescript-json-schema": "^0.53.0"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "node_modules/minecraft-packets/minecraft-packets/data/pc/1.8",
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "dist/"
  }
}
